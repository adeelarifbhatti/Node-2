<!DOCTYPE html>
<html>
	<head>
	<meta charset="utf-8">
	
	
 <!--bower:css-->
 <link rel="stylesheet" href="/lib/bootstrap/dist/css/bootstrap.min.css" />
 <link rel="stylesheet" href="/lib/font-awesome/css/font-awesome.min.css" />
 <!--endbower-->
    
 

    <!--inject:css-->
   
    <link rel="stylesheet" href="/css/admin.css">
   
    <link rel="stylesheet" href="/css/main.css">
   
    <!--endinject-->
  

 </head>

 <body>

  <div id="wrapper">
  

  <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">

  <div class="navbar-header">
   
                
                <a class="navbar-brand" href="/"> <%=title%> </a>
            </div>



                
                <div class="collapse navbar-collapse navbar-ex1-collapse">
                <ul class="nav navbar-nav side-nav">
                    <li class="active">
                    <% for(var i=0; i<sideMenu.length;i++) {%>
                    	<li><a href="<%=sideMenu[i].Link%>"> <i class="fa fa-fw fa-hand-o-right"></i> <%=sideMenu[i].Text%> </a></li>
                    <%}%>
                    

                <!-- Page Heading -->
                <div class="row">
                    <div class="col-lg-12">
                        <h1 class="page-header">
                            Dashboard <small>Statistics Overview</small>
                        </h1>
                        <form name="login" action= "/auth/signin" method="post">
                        <ul>
                        Sign In
                            <li>
                            UserName <input   name="UserName" id="Username">
                            </li>

                            <li>
                              Password <input name="Password" id="password">
                            </li>
                            <li>
                                <input type="submit" value="Sign In">
                            </li>

                            </ul>
                            </form>
                        <form name="login" action= "/auth/signup" method="post">
                        <ul>
                        Sign Up
                            <li>
                            UserName <input   name="UserName" id="Username">
                            </li>

                            <li>
                              Password <input name="Password" id="password">
                            </li>
                            <li>
                                <input type="submit" value="Sign Up">
                            </li>

                            </ul>
                            </form>

                            

                        <ol class="breadcrumb">
                            <li class="active">
                                
                            </li>
                        </ol>
                    </div>
                </div>

            </div>



 <!-- <div class="wrapper">
 	

 </div> -->

    <!--bower:js-->
    <script src="/lib/jquery/dist/jquery.js"></script>
    <script src="/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="/lib/tether/dist/js/tether.js"></script>
    <!--endbower-->
   <!--inject:js-->
   <script src="/js/scripts.js"></script>
   <!--endinject-->

               

</body>
</html>